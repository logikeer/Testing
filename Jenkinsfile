pipeline {
    agent any
    stages {
        stage('Example 1') {
            steps {
                echo "Running ${env.BUILD_ID} on ${env.JENKINS_URL}"
            }
        }
        stage('Example 2') {
            steps {
                echo "This is example 2"
            }
        }
        stage('Example 3') {
            steps {
                echo "it is for the branch Blue Ocean"
            }
        }
    }
}